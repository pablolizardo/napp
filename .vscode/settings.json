{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "node_modules": true,
    ".next": true,
    "next-env.d.ts": true,
    ".gitignore": true,
    "README.md": true,
    "tsconfig.json": true,
    ".eslintrc.json": true,
    "postcss.config.js": true,
    ".babel.rc": true,
    "bun.lockb": true,
    "package-lock.json": true,
    ".vscode": true,
    "jest.config.js": true,
    "components.json": true,
    "next.config.mjs": true,
    "package.json": true,
    "**/_tmp":true
    // "tailwind.config.ts": true,
  },
  "tailwindCSS.includeLanguages": {
    "html": "html",
    "javascript": "javascript",
    "css": "css",
    
},
"editor.quickSuggestions": {
    "strings": true
},
"json.schemas": [
  {
      "fileMatch": ["./xt.config.json"],
      "url": "./src/xt/types/xt-config.schema.json"
  }
]

}
